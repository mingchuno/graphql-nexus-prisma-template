datasource db {
  provider = "postgresql"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id        String   @default(cuid()) @id
  name      String
  email     String   @unique
  birthDate DateTime
  posts     Post[]
}

model Post {
  id      String @default(cuid()) @id
  authors User[] @relation(references: [id])
  title   String
  content String?
  published Boolean @default(false)
}